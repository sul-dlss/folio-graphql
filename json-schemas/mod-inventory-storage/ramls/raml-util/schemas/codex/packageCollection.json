{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "description": "Collection of packages",
  "properties": {
    "packages": {
      "type": "array",
      "description": "Collection of packages",
      "items": {
        "type": "object",
        "$ref": "package.json"
      }
    },
    "resultInfo": {
      "description": "Result information schema",
      "$ref": "../resultInfo.schema"
    }
  },
  "required": [
    "packages",
    "resultInfo"
  ]
}
